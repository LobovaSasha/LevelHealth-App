<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/calendar_background" >

    <CalendarView
        android:id="@+id/calendarView"
        android:background="@drawable/calendarstyle"
        android:layout_width="347sp"
        android:layout_height="308sp"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="207sp"
        android:elevation="10sp" />

    <ImageButton
        android:id="@+id/BackButtonCalendar"
        android:onClick="GoToMainActivity"
        android:layout_width="24sp"
        android:layout_height="24sp"
        android:background="@drawable/back_button"
        android:layout_marginStart="32sp"
        android:layout_marginTop="25sp" />

    <ImageButton
        android:id="@+id/MenuButtonCalendar"
        android:onClick="GoToMenuActivity"
        android:layout_width="24sp"
        android:layout_height="24sp"
        android:background="@drawable/menu_button"
        android:layout_marginStart="355sp"
        android:layout_marginRight="32dp"
        android:layout_marginTop="25dp" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="605sp"
        android:text="TextView" />

</RelativeLayout>
